{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "**/*.js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "application/javascript; charset=utf-8"
          }
        ]
      },
      {
        "source": "**/*.css",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/css; charset=utf-8"
          }
        ]
      },
      {
        "source": "**/*.html",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/html; charset=utf-8"
          }
        ]
      },
      {
        "source": "**/*.@(ttf|woff|woff2|otf)",
        "headers": [
          {
            "key": "Content-Type",
            "value": "font/ttf; charset=utf-8"
          }
        ]
      }
    ],
    "rewrites": [
      { "source": "/Account page/**", "destination": "/Account page/account.html" },
      { "source": "/Closet page/closet.html", "destination": "/Closet page/closet.html" },
      { "source": "/Closet page/bottoms.html", "destination": "/Closet page/bottoms.html" },
      { "source": "/Closet page/kopfbedeckung.html", "destination": "/Closet page/kopfbedeckung.html" },
      { "source": "/Closet page/schuhe.html", "destination": "/Closet page/schuhe.html" },
      { "source": "/Closet page/tops.html", "destination": "/Closet page/tops.html" },
      { "source": "/Fits page/**", "destination": "/Fits page/fits.html" },
      { "source": "/Upload/**", "destination": "/Upload/upload.html" },
      { "source": "/Firebase/**", "destination": "/Firebase/fire.js" },
      { "source": "**", "destination": "/Homepage/index.html" }
    ]
  }
}